<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.Krishna.Quiz_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quiztime"
        android:label="Quiz Time"
        android:roundIcon="@mipmap/quiztime"
        android:supportsRtl="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.Krishna.Quiz_app.Act.ResultActivity" android:theme="@style/AppTheme"/>
        <activity
            android:name="com.Krishna.Quiz_app.Act.StartQuizeActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.Krishna.Quiz_app.MainActivity"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>